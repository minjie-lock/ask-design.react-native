{
  "name": "@ask-design/react-native",
  "version": "0.0.17",
  "private": false,
  "type": "module",
  "scripts": {
    "android": "react-native run-android --config metro.config.cjs",
    "ios": "react-native run-ios --config metro.config.cjs",
    "lint": "eslint .",
    "start": "react-native start --config metro.config.cjs",
    "test": "jest",
    "build": "rslib build"
  },
  "main": "./dist/index.js",
  "author": {
    "name": "Lock",
    "email": "minjie.lock@gmail.com",
    "url": "https://github.com/minjie-lock"
  },
  "keywords": [
    "react",
    "react-native"
  ],
  "bugs": {
    "url": "https://github.com/minjie-lock/ask-design.react-native",
    "email": "minjie.lock@gamil.com"
  },
  "license": "BSD-3-Clause",
  "description": "Ask Design for React Native",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minjie-lock/ask-design.react-native.git"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/src/index.d.ts"
    },
    "./configuration": {
      "default": "./react-native.config.cjs"
    },
    "./metro": {
      "default": "./dist/metro/index.cjs",
      "require": "./dist/metro/index.cjs",
      "types": "./dist/src/metro/index.d.ts",
      "import": "./dist/metro/metro.js"
    }
  },
  "files": [
    "dist",
    "react-native.config.cjs",
    "README.md"
  ],
  "dependencies": {
    "@ant-design/icons-react-native": "^2.3.2",
    "@rsbuild/plugin-react": "^1.3.4",
