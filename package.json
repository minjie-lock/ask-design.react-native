    "ahooks": "^3.9.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "bignumber.js": "^9.3.1",
    "dayjs": "^1.11.13",
    "rc-field-form": "^2.7.0",
    "react-native-fast-image": "^8.6.3"
  },
  "peerDependencies": {
    "react": "^19.1.0",
